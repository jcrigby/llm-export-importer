{
  "name": "llm-export-importer",
  "version": "0.1.0",
  "description": "Extract and organize writing content from AI chat platform exports",
  "main": "dist/index.js",
  "bin": {
    "llm-import": "./dist/cli/index.js",
    "llm-export": "./dist/cli/simple.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "llm",
    "ai",
    "chatgpt",
    "claude",
    "gemini",
    "export",
    "import",
    "writing",
    "content",
    "organization",
    "cli"
  ],
  "author": "jcrigby",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcrigby/llm-export-importer.git"
  },
  "bugs": {
    "url": "https://github.com/jcrigby/llm-export-importer/issues"
  },
  "homepage": "https://github.com/jcrigby/llm-export-importer#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "yaml": "^2.3.4",
    "yauzl": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  }
}
